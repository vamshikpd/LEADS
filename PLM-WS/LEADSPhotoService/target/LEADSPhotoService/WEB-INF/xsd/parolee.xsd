			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Offense">
		<xsd:complexType>
			<xsd:annotation>
				<xsd:documentation>Parolee Offense</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence>
