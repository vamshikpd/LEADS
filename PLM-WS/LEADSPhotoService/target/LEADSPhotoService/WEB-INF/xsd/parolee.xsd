		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Job">
		<xsd:complexType>
			<xsd:annotation>
				<xsd:documentation>Parolee Job Details</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence>
				<xsd:element ref="nc:Employer"/>
