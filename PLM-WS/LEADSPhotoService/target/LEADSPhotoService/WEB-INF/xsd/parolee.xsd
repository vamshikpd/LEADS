	<xsd:element name="Address">
		<xsd:complexType>
			<xsd:annotation>
				<xsd:documentation>A data type for an address.</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence>
				<xsd:element ref="nc:EffectiveDate"/>
				<xsd:element ref="nc:AddressRecipientName"/>
				<xsd:element ref="nc:StreetName"/>
				<xsd:element ref="nc:LocationCityName"/>
				<xsd:element ref="nc:LocationStateName"/>
				<xsd:element ref="nc:LocationCountryName"/>
				<xsd:element ref="nc:LocationPostalCode"/>
				<xsd:element ref="nc:LocationPostalExtensionCode"/>
				<xsd:element ref="nc:TelephoneAreaCodeID"/>
				<xsd:element ref="nc:TelephoneNumberID"/>
				<!-- 
			<xsd:element ref="nc:LocationCountyCode" minOccurs="1" maxOccurs="unbounded"/>
			<xsd:element ref="nc:LocationPostalExtensionCode" minOccurs="1" maxOccurs="unbounded"/>
			 -->
			</xsd:sequence>
				<!-- 
			<xsd:attribute name="type" type="xsd:string" use="required"/>
			 -->
